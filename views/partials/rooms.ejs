<div class='container col-md-8'>
  <%Room.forEach(function(Room){ %>
  <div class='room'>
    <div class='pillheader'>
      <i class='fa fa-map-marker'></i>
      <span>Dungeon Name:
        <%= Room.roomname  %></span>
      <i class='fa fa-minus text-right'></i>
    </div>
    <div class='roomdata'>
      <div class='roomrow'>
        <p>Author:
          <%= Room.authorname %>
        </p>
        <p>Dimensions:
          <%= Room.authorname %>
        </p>
        <p>Date uploaded:
          <%= Room.createdAt %>
        </p>
      </div>
      <div class='col-md-8'>
        <% var temp = sails.config.custom.baseUrl + "/uploads/"+Room.img %>
        <!--<img src='../uploads/<%= Room.img %>'>-->
        <img src='<%= temp %>'>
        <!--<img src='/../dungeon2.jpg'>-->
      </div>
      <div class='col-md-4'>
        <p>
          <%= Room.creatornotes %>
        </p>
      </div>

    </div>
  </div>
  <% }) %>
</div>
